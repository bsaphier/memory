{
  "extends": [ "eslint:recommended", "plugin:react/recommended" ],
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "jest": true,
    "es6": true
  },
  "rules": {
    "no-console": 0,
    "no-unused-vars": 1,
    "semi": [ 1, "always" ],
    "quotes": [ "warn", "single" ],
    "require-await": 1
  }
}